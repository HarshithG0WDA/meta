#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/MyFiles/My-projects/Metaverse/metaverse/apps/http/node_modules/mime/node_modules:/Volumes/MyFiles/My-projects/Metaverse/metaverse/apps/http/node_modules:/Volumes/MyFiles/My-projects/Metaverse/metaverse/apps/node_modules:/Volumes/MyFiles/My-projects/Metaverse/metaverse/node_modules:/Volumes/MyFiles/My-projects/Metaverse/node_modules:/Volumes/MyFiles/My-projects/node_modules:/Volumes/MyFiles/node_modules:/Volumes/node_modules:/node_modules:/Volumes/MyFiles/My-projects/Metaverse/metaverse/apps/http/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/MyFiles/My-projects/Metaverse/metaverse/apps/http/node_modules/mime/node_modules:/Volumes/MyFiles/My-projects/Metaverse/metaverse/apps/http/node_modules:/Volumes/MyFiles/My-projects/Metaverse/metaverse/apps/node_modules:/Volumes/MyFiles/My-projects/Metaverse/metaverse/node_modules:/Volumes/MyFiles/My-projects/Metaverse/node_modules:/Volumes/MyFiles/My-projects/node_modules:/Volumes/MyFiles/node_modules:/Volumes/node_modules:/node_modules:/Volumes/MyFiles/My-projects/Metaverse/metaverse/apps/http/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
